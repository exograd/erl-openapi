{cover_enabled, true}.

{dialyzer, [{plt_extra_apps, []},
            {warnings, [unknown]}]}.

{erl_opts, [debug_info]}.

{deps,
 [{rebar3_format,
   {git, "https://github.com/AdRoll/rebar3_format.git", {branch, "main"}}},
  {mustache,
   {git, "https://github.com/exograd/erl-mustache.git", {branch, "master"}}},
  {uri,
   {git, "https://github.com/exograd/erl-uri.git", {branch, "master"}}},
  {json,
   {git, "https://github.com/exograd/erl-json.git", {branch, "master"}}},
  {jsv,
   {git, "https://github.com/exograd/erl-jsv.git", {branch, "master"}}}]}.

{shell, [{apps, [openapi]}]}.
